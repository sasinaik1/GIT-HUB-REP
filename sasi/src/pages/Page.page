				<apex:inputField value="{!BLND_DFFF_Page__c.BLND_Extra_Field__c}" required="false"/>
				<apex:inputField value="{!BLND_DFFF_Page__c.BLND_Flag1__c}" required="false"/>
				<apex:inputField value="{!BLND_DFFF_Page__c.BLND_Flag2__c}" required="false"/>
				<apex:inputField value="{!BLND_DFFF_Page__c.BLND_Flag3__c}" required="false"/>
				<apex:inputField value="{!BLND_DFFF_Page__c.BLND_Flag4__c}" required="false"/>
				<apex:inputField value="{!BLND_DFFF_Page__c.BLND_Keywords__c}" required="false"/>
