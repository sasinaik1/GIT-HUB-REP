				<apex:inputField value="{!BLND_DFFF_Page__c.BLND_Type__c}" required="false"/>
				<apex:inputField value="{!BLND_DFFF_Page__c.BLND_Version__c}" required="false"/>
			</apex:pageBlockSection>

		</apex:pageBlock>
	</apex:form>
